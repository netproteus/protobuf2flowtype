{
  "name": "protobuf2flowtype",
  "version": "1.0.2",
  "description": "Given protobuf file will generate node packages which export protobufjs builders annotated with flowtype type anotations",
  "main": "index.js",
  "dependencies": {
    "protobufjs": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-generator": "^6.14.0",
    "babylon": "^6.9.1",
    "codecov": "^1.0.1",
    "eslint": "^3.5.0",
    "eslint-plugin-flowtype": "^2.17.1",
    "fs-extra": "^0.30.0",
    "mustache": "^2.2.1",
    "nodeunit": "^0.10.2",
    "nyc": "^8.3.0",
    "yargs": "^5.0.0"
  },
  "scripts": {
    "test": "eslint build/ && nyc nodeunit tests/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netproteus/protobuf2flowtype.git"
  },
  "author": "William Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netproteus/protobuf2flowtype/issues"
  },
  "homepage": "https://github.com/netproteus/protobuf2flowtype#readme",
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "protobuf",
    "proto",
    "flow",
    "flowtype"
  ],
  "nyc": {
    "include": [
      "build/**/*.js*"
    ],
    "exclude": [
      "tests/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": true,
    "all": true
  }
}
